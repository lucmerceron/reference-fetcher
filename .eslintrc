{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "__DEV__": true
  },
  "env": {
    "node": true,
    "browser": true,
    "jasmine": true,
    "jest": true
  },
  "rules": {
    "arrow-parens": "off",
    "generator-star-spacing": "off",
    "import/extensions": "off",
    "import/no-extraneous-dependencies": "off",
    "no-console": 0,
    "global-require": 0,
    "semi": [1, "never"],
    "max-len": ["error", 140],
    "jsx-a11y/no-static-element-interactions": 0,
    "jsx-a11y/img-has-alt": "off"
  },
  "plugins": [
    "jest",
    "jasmine"
  ]
}